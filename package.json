{
  "name": "electron-Angular2-GitHub",
  "version": "1.0.0",
  "description": "Simple app with electron - Angular2 - Typescript",
  "main": "electron/main.js",
  "scripts": {
    "bundle": "webpack --config webpack/webpack.config.js --inline --colors --progress --display-error-details",
    "start": "set NODE_ENV=dev && electron . --github",
    "package": "electron-packager . ElectronAngular2 --ignore=forms --ignore=logs --ignore=webpack --ignore=.idea --ignore=.gitignore --ignore=tsconfig.json --prune --platform=win32 --arch=x64 --appVersion=0.2.1 --appCopyright=DM Inc. --buildVersion=0.2.2 --icon=./views/app.ico --overwrite --out=./build --production --asar=true"
  },
  "author": "qudrat",
  "license": "MIT",
  "dependencies": {
    "async": "1.5.2",
    "bluebird": "3.5.0",
    "body-parser": "1.17.2",
    "express": "4.15.3",
    "github": "9.2.0",
    "morgan": "1.8.2",
    "parse-link-header": "1.0.1",
    "request": "2.81.0",
    "socket.io": "1.4.8",
    "socket.io-client": "1.5.1",
    "underscore": "1.8.3",
    "winston": "2.2.0",
    "winston-daily-rotate-file": "1.3.1"
  },
  "devDependencies": {
    "electron": "1.6.10",
    "electron-packager": "8.7.0",
    "@types/node": "6.0.88",
    "@types/core-js": "0.9.35",
    "@types/underscore": "1.8.2",
    "@types/socket.io-client": "1.4.30",
    "angular2-template-loader": "0.4.0",
    "angular2-router-loader": "0.3.5",
    "awesome-typescript-loader": "3.2.3",
    "assets-webpack-plugin": "3.5.1",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.26.4",
    "css-to-string-loader": "0.1.3",
    "node-sass": "4.5.3",
    "url-loader": "0.5.9",
    "resolve-url-loader": "1.6.1",
    "sass-loader": "4.1.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.30.1",
    "json-loader": "0.5.7",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "style-loader": "0.13.2",
    "to-string-loader": "1.1.5",
    "typescript": "2.0.10",
    "webpack": "1.15.0"
  }
}
